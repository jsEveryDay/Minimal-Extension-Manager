{
  "manifest_version": 2,
  "name": "Extension Manager",
  "version": "1.3",
  "author": "jsEveryDay",
  "description": "Extension Manager",
  "browser_action": {
    "default_title": "Manager",
    "default_popup": "menu.html"
  },
  "icons": {
      "16": "images/16.png",
      "48": "images/48.png",
    "128": "images/128.png"
   },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [ "management" ]
}